{
    "name": "jamielsharief/custom-installer",
    "description": "A custom installer for composer packages",
    "type": "composer-installer",
    "license": "MIT",
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "CustomInstaller\\Composer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CustomInstaller\\Test\\Composer\\": "tests/"
        }
    },
    "extra": {
        "class": "CustomInstaller\\Composer\\PluginInstaller"
    }
}